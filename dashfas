    TextDisappear = true,
    Callback = function(text)
        defaultMessage4 = text
    end
})

Tab:AddToggle({
    Name = "Auto Equip 'Crash' Tools",
    Default = false,
    Callback = function(state)
        isEquipping = state
        if state then
            -- Start the tool equipping loop
            EquipLightsahTools()
        end
    end
})

