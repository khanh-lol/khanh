--Crash Tab

local function ExecuteChatCommands4(message)
    while isExecuting4 do
        local player = game.Players
        local bkitbMessage = "bkit " .. message .. ""
        player:Chat(bkitbMessage)
        wait(0.001)  -- Adjust delay as needed
